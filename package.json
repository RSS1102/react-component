    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.34.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.0.0"
  }
